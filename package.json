{
  "name": "@nosana/deployment-manager",
  "version": "0.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm run prepare && npm run patch && npm run format && tsc",
    "generate:openapi-docs": "node --no-warnings --loader ts-node/esm scripts/generateOpenAPIDocumentation.ts",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "format": "npm run eslint",
    "format:fix": "npm run eslint:fix",
    "patch": "ts-patch install && typia patch",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.0.14",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^4.0.3",
    "@vitest/ui": "^4.0.3",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "^5.8.3",
    "vitest": "^4.0.3"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/middie": "^9.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@nosana/sdk": "0.4.55",
    "@sinclair/typebox": "^0.34.38",
    "@solana/web3.js": "^1.98.2",
    "cron-parser": "^5.3.0",
    "fastify": "^5.4.0",
    "mongodb": "^4.17.2",
    "openapi-fetch": "^0.14.0",
    "typia": "^9.5.0"
  },
  "overrides": {
    "@nosana/sdk": {
        "axios": "^1.12.0"
    },
    "typia": {
        "tmp": "^0.2.4"
    }
  }
}
